{
	"name": "phpwatch/phptoken-polyfill",
	"description": "A polyfill for PHP 8.0's PhpToken class",
	"type": "library",
	"keywords": ["polyfill", "php8", "tokenizer"],
	"homepage": "https://php.watch/versions/8.0/PhpToken",
	"require": {
		"php": "^7.1",
		"ext-tokenizer": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.5.20 || ^8.5.3 || ^9.1.1",
		"ayesh/php-timer": "^1.1.7 || ^2.0"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"PHPWatch\\PhpToken\\Tests\\": "tests/"
		}
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Ayesh Karunaratne",
			"email": "ayesh@ayesh.me"
		}
	],
	"minimum-stability": "stable",
	"support": {
		"issues": "https://github.com/phpwatch/phptoken-polyfill/issues",
		"docs": "https://github.com/phpwatch/phptoken-polyfill/blob/master/README.md",
		"source": "https://github.com/phpwatch/phptoken-polyfill.git"
	}
}
